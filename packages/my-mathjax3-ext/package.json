{
  "name": "mymathjax3ext",
  "version": "0.2.1",
  "description": "A customized MathJax 3 math renderer for JupyterLab",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*.d.ts",
    "lib/*.js",
    "style/*.css"
  ],
  "directories": {
    "lib": "lib/"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "mathjax"
  ],
  "dependencies": {
    "@jupyterlab/application": "^0.18.0",
    "@jupyterlab/rendermime": "^0.18.0",
    "mathjax3": "3.0.0-beta.1"
  },
  "devDependencies": {
    "rimraf": "^2.6.2",
    "typescript": "~2.9.2"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "generate:css": "node scripts/generate-css.js",
    "prepack": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "jupyterlab": {
    "extension": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sparkboom/my-mathjax3-ext.git"
  },
  "author": "Project Jupyter",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/sparkboom/my-mathjax3-ext/issues"
  },
  "homepage": "https://github.com/sparkboom/my-mathjax3-ext"
}
